{
    "name": "igorsgm/laravel-likeable",
    "description": "Make Laravel Eloquent models Likeable & Dislikeable in a minutes!",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "igorsgm",
        "cog",
        "laravel",
        "eloquent",
        "trait",
        "likeable",
        "likable",
        "like",
        "dislike",
        "favorite",
        "favourite",
        "rate",
        "rating",
        "emotion",
        "star",
        "kudos"
    ],
    "authors": [
        {
            "name": "Igor Moraes",
            "email": "igor.sgm@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/igorsgm/laravel-likeable",
    "support": {
        "email": "igor.sgm@gmail.com",
        "issues": "https://github.com/igorsgm/laravel-likeable/issues",
        "wiki": "https://github.com/igorsgm/laravel-likeable/wiki",
        "source": "https://github.com/igorsgm/laravel-likeable",
        "docs": "https://github.com/igorsgm/laravel-likeable/wiki"
    },
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/database": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19.0",
        "mockery/mockery": "^1.5.0",
        "orchestra/database": "^v3.8.4",
        "orchestra/testbench": "^v6.24.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Igorsgm\\Likeable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Igorsgm\\Likeable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Igorsgm\\Likeable\\Providers\\LikeableServiceProvider"
            ]
        }
    },
    "suggest": {
        "cybercog/laravel-love": "Completely refactored Laravel Likeable package."
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
